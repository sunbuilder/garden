<!--logs.wxml-->
 <form bindsubmit="reg">

<view class="productStatus">
  <span>
    <em><button  form-type="submit"class="but">删除</button></em>
  </span>
</view>


<view class="con">

  <view class="userinfo">
    
    
      <image class="userinfo-avatar" src="{{diary.diaryImage}}" mode="cover"></image>
      <text class="userinfo-nickname" ></text>
    
  </view>

 </view>

  <view class="board">
 
  <view class="board-item">
  <image class="userinfo-avatar1"src="../../image/p1.png" background-size="cover"></image>
     <view class="board-text"> <text >植物姓名</text></view>
       <view ><input type="text" name="plantname"placeholder="未填写"class="x" value='{{diary.diaryPlantname}}' /></view> 
  </view>
  
   </view>
   <view class="board">
 
  <view class="board-item">
  <image class="userinfo-avatar1"src="../../image/p2.png" background-size="cover"></image>
      <view class="board-text"> <text >栽培空间</text></view>
       <view ><input  name="diaryspace"placeholder="未填写"class="x" value='{{diary.diarySpace}}' /></view> 
  </view>
  
   </view>
   <view class="board">
  
  <view class="board-item">
  <image class="userinfo-avatar1"src="../../image/p3.png" background-size="cover"></image>
     <view class="board-text"> <text >栽培方式</text></view>
       <view ><input name="diarymethod"placeholder="未填写"class="x" value='{{diary.diaryMethod}}'/></view> 
  </view>
  
   </view>
   <view class="board">
  
  <view class="board-item">
  <image class="userinfo-avatar1"src="../../image/p4.png" background-size="cover"></image>
     <view class="board-text"> <text >所在城市</text></view>
       <view ><input name="diarycity"placeholder="未填写"class="x" value="{{diary.diaryCity}}"/></view> 
  </view>
  
   </view>
 

 <view class="userinfo">
     <text class="userinfo-nickname">--养殖经验--</text>
  </view>
   <view class="contain">

    <view class="">
    <image class="userinfo-avatar1"src="../../image/sun.png" background-size="cover"></image>
     <view class="contain"><text class="style">{{diary.diarySun}}</text></view>
    </view>
   
   <view class="">
    <image class="userinfo-avatar1"src="../../image/rain.png" background-size="cover"></image>
    <view class="contain"><text class="style">{{diary.diaryWater}}</text></view>
    </view>
   
  <view class="">
    <image class="userinfo-avatar1"src="../../image/turang.png" background-size="cover"></image>
     <view class="contain"><text class="style">{{diary.diarySoil}}</text></view>
    </view>
 
   <view class="">
    <image class="userinfo-avatar1"src="../../image/time.png" background-size="cover"></image>
    <view class="contain"><text class="style">{{diary.diaryFlowering}}</text></view>
    </view>
   
  
</view>
 </form>
